---
layout: null
sitemap: false
---

<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">

    {% for page in site.pages %}
    {% if page.sitemap %}
    <url>
        <loc>https://stackacademy.tv{{page.url}}</loc>
    </url>
    {% endif %}
    {% endfor %}

    {% for post in site.posts %}
    {% if page.sitemap %}
    <url>
        <loc>{{post.url}}</loc>
    </url>
    {% endif %}
    {% endfor %}

</urlset>
